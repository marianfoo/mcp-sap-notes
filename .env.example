# SAP Certificate Configuration (Required)
# Path to your SAP Passport certificate file (.pfx format)
PFX_PATH=./certs/sap.pfx
# Passphrase for your SAP Passport certificate
PFX_PASSPHRASE=your_certificate_passphrase

# Authentication Configuration (Optional)
# Maximum age of cached authentication in hours
MAX_JWT_AGE_H=12

# Browser Configuration (Optional)
# Set to 'true' to run browser in headful mode (useful for debugging)
HEADFUL=false

# Logging Configuration (Optional)
# Log level: debug, info, warn, error
LOG_LEVEL=info
